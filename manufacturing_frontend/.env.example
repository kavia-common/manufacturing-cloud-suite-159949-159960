# manufacturing_frontend - .env.example
# Copy to .env and set values for local/dev.

# Base URL for the backend API (used by fetch)
VITE_API_BASE_URL=http://localhost:8000

# Base URL for WebSocket connections (ws or wss)
# If omitted, client will derive from window.location with ws/wss protocol.
VITE_WS_BASE_URL=ws://localhost:8000
